# Ottieni il nodo sorgente
source_node = nuke.toNode('MotionBlur_X')

# Ottieni il nodo destinazione
destination_node = nuke.toNode('MotionBlur_X')

# Nome del knob nel nodo sorgente
source_knob_name = 'a'

# Nome del knob nel nodo destinazione
destination_knob_name = 'shutterSamples'

# Crea l'espressione per collegare i due knob
expression = '{}.{}'.format(source_node.name(), source_knob_name)

# Collega l'espressione al knob del nodo destinazione
destination_node[destination_knob_name].setExpression(expression) 




